CSON_FILE=../grammars/html\ (utl).cson

grammars: $(CSON_FILE)

$(CSON_FILE): generate_html_utl_grammar.coffee
	echo "# This file generated by atom-syntax-tools (https://www.npmjs.com/package/atom-syntax-tools)" > "$@"
	echo >> "$@"
	coffee generate_html_utl_grammar.coffee >> "$@"

.PHONY: clean

clean:
	rm $(CSON_FILE)
